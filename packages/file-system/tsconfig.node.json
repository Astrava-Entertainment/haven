{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "~": [
        "."
      ],
      "~/*": [
        "./src/*"
      ],
      "~~": [
        "."
      ],
      "~~/*": [
        "./src/*"
      ],
      "examples": [
        "./examples"
      ],
      "examples/*": [
        "./examples/*"
      ]
    },
  },
  "include": [
    "vite.config.ts"
  ]
}
